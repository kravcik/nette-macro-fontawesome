{
    "name": "kravcik/nette-macro-fontawesome",
    "type": "library",
    "description": "Simple macro for Nette framework, which allow short entry of font-awesome icons.",
    "keywords": ["nette", "macro", "font awesome"],
    "homepage": "https://github.com/kravcik/nette-macro-fontawesome",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/kravcik/nette-macro-fontawesome/issues"
    },

    "require": {
        "php": "^8.0",
        "latte/latte": "^2",
        "nette/utils": "^3"
    },

    "require-dev": {
        "tracy/tracy": "*",
        "nette/tester": "*",
        "modul-is/coding-standard": "v3.x-dev",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-nette": "^0.12"
    },

    "autoload": {
        "psr-4": {
            "Kravcik\\Macros\\": "src/"
        }
    }
}